{
    "name": "emscript-sdk",
    "version": "26.1.0",
    "bin": {
        "emscript": "./bin/main.js"
    },
    "dependencies": {
        "emscript-cli": "https://github.com/em-foundation/npm-packages/releases/download/resources/emscript-cli-26.1.0.202502161134.tgz",
        "segger-arm": "https://github.com/em-foundation/npm-packages/releases/download/resources/segger-arm-8.22.1.tgz",
        "ti-uniflash": "https://github.com/em-foundation/npm-packages/releases/download/resources/ti-uniflash-8.7.0.tgz"
    },
    "scripts": {
        "postinstall": "cp -r ./* ../../ && cd ../../ && npx emscript-cli --root=. config",
        "verify-build": "cd workspace; npx emscript-cli build --unit em.core/em.examples.basic/Ex02_BlinkerP.em.ts",
        "verify-load": "cd workspace; npx emscript-cli build --unit em.core/em.examples.basic/Ex02_BlinkerP.em.ts --load"
    }
}